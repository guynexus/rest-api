{
  "compilerOptions": {
    // "lib": [],
    "lib": ["es2018", "es5", "es6"],
    // "baseurl":".",
    "paths": {
      "@/types/*":["./src/types/*"],
      "*":["./src/types/*"]
    },
    "typeRoots": ["./src/types/"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": false,
  }
}